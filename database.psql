CREATE TABLE user_balance (
  user_id BIGINT,
  balance INTEGER,
  PRIMARY KEY (user_id)
);

CREATE TABLE user_fish_inventory (
  user_id BIGINT,
  fish TEXT,
  fish_name TEXT
);

CREATE TABLE user_item_inventory (
  user_id BIGINT,
  cfb INT,
  ufb INT,
  rfb INT,
  efb INT,
  lfb INT
  PRIMARY KEY (user_id)
);

